
<head>
    <link rel="stylesheet" href="/stylesheets/style1.css">
    <link rel="stylesheet" href="/stylesheets/style2.css">
</head>
<body>
    <div class="nav">
        <a href="/home">
        <div class="back">
            <img src="/icons/back-button.png" alt="logout" class="back-icon">
        </div>
        </a>
        <div>
            <img src="/images/SafeNet.png" alt="logo" class="logo">
        </div>
    </div>
<div class="main">
    <h1 class="title">Organization Verification</h1>
    <table class="table">
        <tr>
            <thead>
                <th>Organization Name</th>
                <th>Email</th>
                <th>Phone Number</th>
                <th>Location</th>
                <th></th>
        </tr>
        </thead>
        <tbody>
            {{#each data}}
            <tr>
                <td>{{this.name}}</td>
                <td>{{this.location}}</td>
                <td>{{this.Phone}}</td>
                <td>{{this.email}}</td>
                <td>
                    <button class="approve-btn">Approve</button>
                    <button class="reject-btn">Reject</button>
                </td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>
</body>
